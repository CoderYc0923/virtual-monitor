{
  "name": "@virtual-monitor/monorepo",
  "version": "0.2.0",
  "description": "前端监控插件,支持[行为、性能、异常、请求、资源、路由、曝光]监控",
  "private": true,
  "scripts": {
    "docs": "pnpm run -C docs docs",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:publish": "sh scripts/publish-docs.sh",
    "build": "pnpm run update && esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "build:rollup-watch": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c -w",
    "update": "esno scripts/update.ts",
    "update:full": "pnpm run update",
    "clean": "rimraf --glob dist types \"packages/*/dist\"",
    "release": "bumpp --execute=\"npm run release:prepare\" --no-push --no-commit --no-tag",
    "release:prepare": "npm run update",
    "publish": "esno scripts/publish.ts",
    "test:vue3": "pnpm run -C examples/vue3 start",
    "example:publish": "esno scripts/publish-examples.ts",
    "example:publish-vue3": "sh scripts/examples/vue3.sh"
  },
  "keywords": [
    "埋点",
    "性能",
    "异常",
    "性能埋点",
    "异常埋点",
    "前端埋点"
  ],
  "author": "yechao email:1151062047@qq.com",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.10",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "bumpp": "^9.0.0",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "esbuild": "0.17.14",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "ohmyfetch": "^0.4.21",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.2",
    "vitepress": "1.0.0-beta.5"
  }
}
